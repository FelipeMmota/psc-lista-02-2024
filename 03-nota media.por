programa {
 
  funcao inicio() {
    inteiro notaUm
    escreva ("A primeira nota: ")
    leia (notaUm)
    inteiro notaDois
    escreva ("A segunda nota: ")
    leia (notaDois)
    inteiro notaTreis
    escreva ("A tericeia nota: ")
    leia (notaTreis)
    inteiro notaQuatro
    escreva ("A quarta nota: ")
    leia (notaQuatro)
    real media = (notaUm + notaDois + notaTreis + notaQuatro) /4   
    escreva ("A média das notas é " + media)
  }
}
